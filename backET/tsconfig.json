{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "dist",
    "rootDir": "api",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    
    "allowImportingTsExtensions": true,
    "noEmit":true
  },
  "include": ["api/**/*.ts"],
  "exclude": ["node_modules"]
}
